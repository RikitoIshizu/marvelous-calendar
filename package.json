{
	"name": "my-app",
	"version": "0.1.0",
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"dev": "next dev -H 0.0.0.0",
		"tw": "tailwindcss build src/styles/styles.css -o src/styles/styles_dev.css",
		"twbuild": "cross-env NODE_ENV=production tailwindcss build src/styles/styles.css -o src/styles/styles_dev.css",
		"build": "next build",
		"start": "next start",
		"codecheck": "tsc",
		"test": "vitest",
		"lint": "npx eslint . --no-cache",
		"lintfix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"reset": "rm -rf .next node_modules && yarn && yarn build",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"check-env": "env-cmd -f .env echo $SUPABASE_PROJECT_ID",
		"generate:supabase-types": "env-cmd -f .env bash -c 'supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" > ./src/types/supabase-ganerated-types.ts 2>&1'"
	},
	"dependencies": {
		"@supabase/auth-ui-react": "^0.4.7",
