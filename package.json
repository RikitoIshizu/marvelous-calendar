		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-storybook": "^0.11.2",
		"jsdom": "^26.0.0",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"storybook": "^8.5.1",
		"tailwindcss": "^3.4.17",
